{
  "name": "ambitful-api",
  "version": "1.0.0",
  "description": "Ambitful AI Backend API",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run clean && npx prisma generate && node ./node_modules/typescript/bin/tsc",
    "start": "node --max-old-space-size=4096 dist/server.js",
    "dev": "npm run lint:fix && npm run docs:generate && npx tsx watch src/server.ts",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:dev": "npm run docs:generate && npm run dev",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "clean": "rimraf dist",
    "type-check": "node ./node_modules/typescript/bin/tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx src/prisma/seed.ts",
    "validate": "npm run type-check && npm run lint",
    "prepare": "npm run prisma:generate",
    "prebuild": "npm run validate",
    "prestart": "npm run build"
  },
  "keywords": [
    "api",
    "backend",
    "ambitful",
    "ai"
  ],
  "author": "Ambitful AI Team",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@langchain/core": "^1.1.8",
    "@langchain/openai": "^1.2.0",
    "@langchain/pinecone": "^1.0.1",
    "@langchain/textsplitters": "^1.0.1",
    "@pinecone-database/pinecone": "^5.1.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "async-retry": "^1.3.3",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "envalid": "^8.1.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "langchain": "^1.2.3",
    "morgan": "^1.10.1",
    "openai": "^6.15.0",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "turndown": "^7.2.2",
    "winston": "^3.19.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/async-retry": "^1.4.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/turndown": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
