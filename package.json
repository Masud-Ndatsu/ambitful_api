{
  "name": "ambitful-api",
  "version": "1.0.0",
  "description": "Ambitful AI Backend API",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "npm run docs:generate",
    "build": "tsc",
    "start": "node dist/server.js",
    "predev": "npm run docs:generate",
    "dev": "tsx watch src/server.ts",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:dev": "npm run docs:generate && npm run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx src/prisma/seed.ts"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "prisma",
    "ai",
    "careers"
  ],
  "author": "Ambitful AI Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "envalid": "^8.1.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.21.0",
    "winston": "^3.19.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "prisma": "^7.2.0",
    "typescript": "^5.9.3"
  }
}
